@{
    Layout = "_LayoutSellers";
}
<style>
    .btn-outline-custom {
        border: #ff5722;
        background: #ff5722;
    }

    .form-control:focus {
        border: 1px solid #ff5722;
        box-shadow: none;
    }

    .btn-outline-custom:hover {
        border: #ff5722;
        background: #ff5722;
    }
</style>

<div class="content">
    <div class="animated fadeIn">
        <div class="row">
            <div class="col-lg-12">
                <div class="card">
                    <div class="card-header">
                        <strong class="card-title" style="font-size: 25px">Danh sách gói dịch vụ đã sử dụng</strong>
                    </div>
                    <div class="card-body">
                        <div class="custom-tab">
                            <div class="tab-content pl-3 pt-2" id="nav-tabContent">
                                <div class="tab-pane fade show active" id="custom-nav-home" role="tabpanel"
                                     aria-labelledby="custom-nav-home-tab">
                                    <form class="form-inline my-2 my-lg-0">
                                        <input class="form-control mr-sm-2" type="search" placeholder="Nhập thông tin cần tìm ..."
                                               aria-label="Search">

                                        <button class="btn btn-outline-primary btn-outline-custom"
                                                type="submit">
                                            <i style="color:#ffffff" class="fa fa-search"></i>
                                        </button>
                                    </form>
                                    <br>
                                    <table id="bootstrap-data-table" class="table table-striped table-bordered">
                                        <thead>
                                            <tr>
                                                <th>ID</th>
                                                <th>Tên gói</th>
                                                <th>Giá</th>
                                                <th>Thời gian đăng ký</th>
                                                <th>Thời gian hết hạng</th>
                                                <th>Tình trạng</th>
                                            </tr>
                                        </thead>
                                        <tbody>
                                            <tr>
                                                <td>12037282</td>
                                                <td>Dimond</td>
                                                <td>10.000.000</td>
                                                <td>10/11/2018</td>
                                                <td>10/12/2018</td>
                                                <td><span class="badge badge-complete">Complete</span></td>
                                            </tr>
                                            <tr>
                                                <td>12037282</td>
                                                <td>Gold</td>
                                                <td>5.000.000</td>
                                                <td>10/11/2018</td>
                                                <td>10/12/2018</td>
                                                <td><span class="badge badge-pending">Pending</span></td>
                                            </tr>
                                            <tr>
                                                <td>12037282</td>
                                                <td>Silver</td>
                                                <td>2.000.000</td>
                                                <td>10/11/2018</td>
                                                <td>10/12/2018</td>
                                                <td><span class="badge badge-pending">Pending</span></td>
                                            </tr>
                                        </tbody>
                                    </table>
                                    <br>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div><!-- /# column -->
        </div>
    </div>
    <!-- /.content -->
</div>